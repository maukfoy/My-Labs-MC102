int main() {
    int a, nmo
    double pcg, pin, ppo, pma, pch, pcn, ncg, mcg, dpcg;

    /* Fazendo leitura dos dados  */
    scanf("%d %d %lf %lf %lf %lf %lf %lf %lf %lf %lf", &a, &nmo, &pcg, &pin, &ppo, &pma, &pch, &pcn, &ncg, &mcg, &dpcg);
    
    /* Calculando valor da media ponderada das provas */
    p = (3 * p1 + 3 * p2 + 4 * p3)/10;

    /* Imprimindo resposta */
    printf("Media aritmetica: %.1f\n", (p + l)/2);
    printf("Media harmonica: %.1f\n", (2 * p * l)/(p + l));
    printf("Media geometrica: %.1f\n", sqrt(p * l));

    return 0;
} 
